  max-width: 1200px;
  margin: 0 auto;
  padding: 20px;
}

#search {
  width: 100%;
  padding: 10px;
  margin-bottom: 20px;
  font-size: 16px;
  border-radius: 5px;
  border: none;
}

.product-grid {
  display: grid;
  grid-template-columns: repeat(5, 1fr);
  gap: 20px;
}

.product {
  background-color: #2A2C34;
  padding: 20px;
  border-radius: 10px;
  text-align: center;
  transition: background-color 0.3s ease;
}

.product:hover {
  background-color: #3B3D47;
